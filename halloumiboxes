#include <bits/stdc++.h>
using namespace std;
 
#define fast_io() ios::sync_with_stdio(false); cin.tie(0);
#define vi vector<int>
#define all(v) (v).begin(), (v).end()
#define pb push_back
#define ll long long
 
const int MOD = 1e9 + 7;
 
int mod_add(int a, int b) { return ((a % MOD) + (b % MOD)) % MOD; }
int mod_mul(int a, int b) { return ((a % MOD) * (b % MOD)) % MOD; }
 
void solve() {
    int n,k;
    cin >> n >> k;
    vi arr(n);
    for(int i=0;i<n;i++){
        cin >> arr[i]; //O(n) SC-//O(n)
    }
   vi copy = arr; // O(n) SC -//O(n)
   sort(all(copy)); //O(nlogn)
   if(copy == arr || k>1){ //O(n)
       cout << "Yes" << "\n";
   }
   else{
       cout << "No" << "\n";
   }
    
}
 
int main() {
    fast_io();
    int t;
    cin >> t;
    while (t--) solve();
    return 0;
}
/*
TIME COMPLEXITY 0(nlog n)
SPACE COMPLEXITY by sorting is O(log n ) and overall O(n)

*/