#include <bits/stdc++.h>
using namespace std;
 
#define fast_io() ios::sync_with_stdio(false); cin.tie(0);
#define vi vector<int>
#define all(v) (v).begin(), (v).end()
#define pb push_back
 
const int MOD = 1e9 + 7;
 
int mod_add(int a, int b) { return ((a % MOD) + (b % MOD)) % MOD; }
int mod_mul(int a, int b) { return ((1LL * (a % MOD) * (b % MOD)) % MOD); }
 
void solve() {
    int n, k;
    cin >> n >> k;
    vi a(n);
    for (int i = 0; i < n; ++i) cin >> a[i];
 int cnt=0
   for(int i=0;i<n;i++){
       if(k==a[i]){
           cnt++;
       }
   }
 
 
    if (cnt>0) cout << "Yes\n";
    else cout << "No\n";
}
 
int main() {
    fast_io();
    int t;
    cin >> t;
    while (t--) solve();
    return 0;
}
/*
TIME-COMPLEXITY:O(n);
SPACE-COMPLEXITY:O(n);
*/